{
    "name": "my-utilities-frontend",
    "version": "1.0.0",
    "description": "The frontend for a web application designed to help you track your utility usage",
    "private": false,
    "author": "Madeleen Roestorff",
    "keywords": [
        "utilities",
        "nextjs"
    ],
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install",
        "validate": "scripts/validate.sh"
    },
    "dependencies": {
        "@date-io/moment": "^2.16.1",
        "@emotion/cache": "latest",
        "@emotion/react": "latest",
        "@emotion/server": "latest",
        "@emotion/styled": "latest",
        "@mui/icons-material": "latest",
        "@mui/material": "latest",
        "@mui/x-date-pickers": "^6.0.1",
        "@next/font": "latest",
        "axios": "^1.3.2",
        "framer-motion": "^9.0.4",
        "http-status-codes": "^2.2.0",
        "moment": "^2.29.4",
        "next": "latest",
        "react": "latest",
        "react-dom": "latest"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@types/react": "latest",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.52.0",
        "eslint": "latest",
        "eslint-config-next": "latest",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.0",
        "jira-prepare-commit-msg": "^1.7.2",
        "prettier": "^2.8.3",
        "typescript": "latest"
    },
    "jira-prepare-commit-msg": {
        "messagePattern": "$J: $M",
        "jiraTicketPattern": "([A-Z]+-\\d+)",
        "commentChar": "#",
        "isConventionalCommit": false,
        "allowEmptyCommitMessage": false,
        "gitRoot": ""
    }
}
